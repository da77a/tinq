<?xml version="1.0" encoding="UTF-8"?>
<types xmlns="http://www.qeo.org/formal/2013/Qeo_Data_Model" version="0.1">
    <include file="qeo_types.xml"/>

    <module name="org::qeo::sample::simplechat">
  	<struct name="ChatMessage" behavior="event">
            <doc>A simple chat message.</doc>  
    	  <member name="from" type="string"> 
    		    <doc>The user sending the message.</doc>
    		</member>
    		<member name="message" type="string"> 
    		    <doc>The message.</doc>
    		</member>
        <member name="bytenumber" type="byte">
            <doc>A 8 bit number</doc>
        </member>
        <member name="int16number" type="int16">
            <doc>A 16 bit number</doc>
        </member>
        <member name="int32number" type="int32">
            <doc>A 32 bit number</doc>
        </member>
        <member name="int64number" type="int64">
            <doc>A 64 bit number</doc>
        </member>
        <member name="floatnumber" type="float32">
            <doc>A 32 bit float number</doc>
        </member>
        <member name="somebool" type="boolean">
            <doc>A boolean</doc>
        </member>
        <member name="UUID" type="nonBasic" nonBasicTypeName="org::qeo::UUID">
            <doc>Some external structure</doc>
        </member>
        <member name="numbersequence" type="int32" sequenceMaxLength="-1">
            <doc>Some sequence of numbers</doc>
        </member>
        <member name="stringsequence" type="string" sequenceMaxLength="-1">
            <doc>Some sequence of strings</doc>
        </member>
        <member name="uuidsequence" type="nonBasic" nonBasicTypeName="org::qeo::UUID" sequenceMaxLength="-1">
            <doc>Some sequence of UUID</doc>
        </member>
    </struct>    
    </module>
</types> 
