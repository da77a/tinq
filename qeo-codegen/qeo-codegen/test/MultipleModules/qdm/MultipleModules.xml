        xsi:schemaLocation="http://www.qeo.org/formal/2013/Qeo_Data_Model qeo_qdm.xsd" version="0.1">
 
  <module name="module::first">
    <struct name="MyStructWithPrimitives" >       
        <doc>struct containing only primitive types </doc> 
        <member name="MyBoolean" type="boolean"/>
        <member name="MyByte" type="byte"/>
        <member name="MyInt16" type="int16"/>
        <member name="MyInt32" type="int32"/>
        <member name="MyInt64" type="int64"/> 
        <member name="MyFloat32" type="float32"/>
        <member name="MyString" type="string"/>
    </struct>
      
    <struct name="Class1">       
       <doc>this is the device ID generated by Qeo. </doc>
       <member name="upper" type="int64" />
       <member name="MyUnbArrayOfStructWithPrimitives" type="nonBasic" nonBasicTypeName="MyStructWithPrimitives" sequenceMaxLength="-1"/>
       <member name="lower" type="int64" />
    </struct>
  </module>

  <module name="module::second">
     <struct name="MyStructWithPrimitives" >       
        <doc>struct containing only primitive types</doc> 
        <member name="MyBoolean" type="boolean"/>
        <member name="MyByte" type="byte"/>
        <member name="MyInt16" type="int16"/>
        <member name="MyInt32" type="int32"/>
        <member name="MyInt64" type="int64"/> 
        <member name="MyFloat32" type="float32"/>
        <member name="MyString" type="string"/>
    </struct>
      
    <struct name="Class1">       
       <doc>this is the device ID 2 generated by Qeo. </doc>
       <member name="upper2" type="int64" />
       <member name="lower2" type="int64" />
    </struct>
  </module>
</types>
