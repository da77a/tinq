<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.qeo.android.service.ui">


    <uses-sdk
        android:minSdkVersion="9"/>


    <application>


        <activity
            android:name="org.qeo.android.security.OtcActivity"
            android:excludeFromRecents="true"
            android:label="@string/app_name"
            android:theme="@style/DialogTheme">
        </activity>
        <activity
            android:name="org.qeo.android.security.ManifestActivity"
            android:excludeFromRecents="true"
            android:noHistory="true"
            android:theme="@style/DialogTheme">
        </activity>


        <activity
            android:name="org.qeo.deviceregistration.ui.WebViewActivity"
            android:hardwareAccelerated="false"
            android:label="@string/app_name"
            android:theme="@style/QeoServiceTheme">
        </activity>
        <activity
            android:name="org.qeo.deviceregistration.ui.ManagementActivity"
            android:label="@string/app_name"
            android:theme="@style/QeoServiceTheme">
        </activity>
        <activity
            android:name="org.qeo.deviceregistration.ui.SettingsActivity"
            android:label="@string/action_settings"
            android:theme="@style/QeoServiceTheme"/>
        <activity
            android:name="org.qeo.android.service.ui.AboutActivity"
            android:label="@string/about">
        </activity>
        <activity
            android:name="org.qeo.android.service.ui.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/QeoServiceTheme">
        </activity>

    </application>

</manifest>
